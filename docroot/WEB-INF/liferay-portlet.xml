<?xml version="1.0"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.1.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_1_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>empty-project</portlet-name>
		<icon>/icon.png</icon>
		<configuration-action-class>br.mp.mprj.util.ConfigurationActionImpl</configuration-action-class>
		<header-portlet-css>/css/main.css</header-portlet-css>
		<header-portlet-css>/css/jquery-ui.css</header-portlet-css>
		<header-portlet-javascript>/js/jquery-1.12.4.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery-ui.min.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.mask.js</header-portlet-javascript>
		<footer-portlet-javascript>/js/main.js</footer-portlet-javascript>
		<css-class-wrapper>empty-project-portlet</css-class-wrapper>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
